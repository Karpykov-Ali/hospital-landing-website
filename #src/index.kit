<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>GoCorona</title>
    <!--CSS-->
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
<div class="wrapper">
    <!-- @import "header.html" -->
    <main class="page">
        <section class="getstarted-page">
            <div class="getstarted-page__container container">
                <div class="getstarted-page__body getstarted-body">
                    <div class="getstarted-body__content getstarted-content">
                        <div class="getstarted-content__text content-text">
                            <h1 class="content-text__title content-text-text__title_blue">
                                Take care of yours familyâ€™s <span>health.</span>
                            </h1>
                            <div class="content-text__block-text">
                                <p class="content-text__text">
                                    All in one destination for COVID-19 health queries. Consult 10,000+ health workers
                                    about
                                    your concerns.
                                </p>
                            </div>
                            <a href="#" class="content-text__button button">
                                <span>
                                    GET STARTED
                                </span>
                            </a>
                        </div>
                    </div>
                    <div class="getstarted-body__image getstarted-image">
                        <img src="img/getstarted/01.svg" alt="doctor image">
                    </div>
                </div>
                <div class="getstarted-page__bottom getstarted-bottom">
                    <div class="getstarted-bottom__icon">
                        <a href="#" class="getstarted-bottom__video-link">
                            <img src="img/icons/play.svg" alt="video">
                        </a>
                    </div>
                    <div class="getstarted-bottom__body video-body">
                        <h3 class="video-body__title">
                            Stay safe with GoCorona
                        </h3>
                        <a href="#" class="video-body__label-link">
                            Watch Video
                        </a>
                    </div>
                </div>
            </div>
        </section>
        <section class="staysafe-page">
            <div class="staysafe-page__container container">
                <div class="staysafe-page__body staysafe-body">
                    <div class="staysafe-body__app staysafe-app">
                        <div class="staysafe-app__image">
                            <img src="img/staysafe/app.svg" alt="phone app">
                        </div>
                        <div class="staysafe-app__item staysafe-app__item_1"></div>
                        <div class="staysafe-app__item staysafe-app__item_2"></div>
                        <div class="staysafe-app__item staysafe-app__item_3"></div>
                    </div>
                    <div class="staysafe-body__content satysafe-content">
                        <div class="staysafe-content__text content-text">
                            <h2 class="content-text__title">
                                Stay safe with Go<span>Corona.</span>
                            </h2>
                            <div class="content-text__block-text">
                                <p class="content-text__text">
                                    24x7 Support and user friendly mobile platform to bring healthcare to your
                                    fingertips. Signup and be a part of the new health culture.
                                </p>
                            </div>
                            <a href="#" class="content-text__button button">
                                <span>
                                    Features
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="experts-page">
            <div class="experts-page__container container">
                <div class="experts-page__stat experts-stat">
                    <div class="experts-stat__body experts-body">
                        <div class="experts-body__column">
                            <h2 class="experts-body__value">
                                2m
                            </h2>
                            <div class="experts-body__label">
                                Users
                            </div>
                        </div>
                        <div class="experts-body__column">
                            <h2 class="experts-body__value">
                                78
                            </h2>
                            <div class="experts-body__label">
                                Countries
                            </div>
                        </div>
                        <div class="experts-body__column">
                            <h2 class="experts-body__value">
                                10,000+
                            </h2>
                            <div class="experts-body__label">
                                Medical experts
                            </div>
                        </div>
                    </div>
                    <div class="experts-stat__decor experts-stat__decor_1"></div>
                    <div class="experts-stat__decor experts-stat__decor_2"></div>
                    <div class="experts-stat__decor experts-stat__decor_3"></div>
                </div>
                <div class="experts-page__bottom experts-bottom">
                    <div class="experts-bottom__text content-text">
                        <h2 class="content-text__title content-text__title_blue">
                            Talk to <span>experts.</span>
                        </h2>
                        <div class="content-text__block-text">
                            <p class="content-text__text">
                                Book appointments or submit queries into thousands of forums concerning health issues
                                and prevention against noval Corona Virus.
                            </p>
                        </div>
                        <a href="#" class="content-text__button button">
                            <span>
                                Features
                            </span>
                        </a>
                    </div>
                    <div class="experts-bottom__video">
                        <iframe src="https://www.youtube.com/embed/OeGpf1MyM2M"
                                title="YouTube video player" frameborder="0"
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                                allowfullscreen>
                        </iframe>
                    </div>
                </div>
            </div>
        </section>
        <section class="healthcare-page">
            <div class="healthcare-page__container container">
                <div class="healthcare-page__text content-text">
                    <h2 class="content-text__title">
                        <span>Healthcare</span> at your Fingertips.
                    </h2>
                    <p class="content-text__text content-text__text_mw">
                        Bringing premium healthcare features to your fingertips. User friendly mobile platform to bring
                        healthcare to your fingertips. Signup and be a part of the new health culture.
                    </p>
                </div>
                <div class="healthcare-page__body healthcare-body">
                    <div class="healthcare-body__column healthcare-body__column_1">
                        <div class="healthcare-body__item">
                            <div class="healthcare-body__icon">
                                <img src="img/healthcare/01.svg" alt="human icon">
                            </div>
                            <h3 class="healthcare-body__title">
                                Symptom Checker
                            </h3>
                            <p class="healthcare-body__text">
                                Check if you are infected by COVID-19 with our Symptom Checker
                            </p>
                        </div>
                    </div>
                    <div class="healthcare-body__column healthcare-body__column_2">
                        <div class="healthcare-body__item">
                            <div class="healthcare-body__icon">
                                <img src="img/healthcare/02.svg" alt="human icon">
                            </div>
                            <h3 class="healthcare-body__title">
                                24x7 Medical support
                            </h3>
                            <p class="healthcare-body__text">
                                Consult with 10,000+ health workers about your concerns.
                            </p>
                        </div>
                    </div>
                    <div class="healthcare-body__column healthcare-body__column_3">
                        <div class="healthcare-body__item">
                            <div class="healthcare-body__icon">
                                <img src="img/healthcare/03.svg" alt="human icon">
                            </div>
                            <h3 class="healthcare-body__title">
                                Conditions
                            </h3>
                            <p class="healthcare-body__text">
                                Check if you are infected by COVID-19 with our Symptom Checker
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <!-- @import "footer.html" -->
</div>
<!--JS-->
<script src="js/main.js"></script>
</body>
</html>